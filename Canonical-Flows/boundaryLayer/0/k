FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 3.75e-3;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            zeroGradient;
    }

    top
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    frontAndBack
    {
		type            empty;   
    }
	
	plate
    {
        type            kqRWallFunction;
        value           $internalField;
    }

}

// ************************************************************************* //
