FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 45.4;

boundaryField
{
    topZ
    {
        type            fixedValue;
        value           uniform 45.4;
    }
    bottomZ
    {
        type            zeroGradient;
    }
    topY
    {
        type            epsilonWallFunction;
        value           uniform 45.4;
    }
    bottomY
    {
        type            epsilonWallFunction;
        value           uniform 45.4;
    }
    topX
    {
        type            epsilonWallFunction;
        value           uniform 45.4;
    }
	bottomX
    {
        type            epsilonWallFunction;
        value           uniform 45.4;
    }
	brick
    {
        type            epsilonWallFunction;
        value           uniform 45.4;
    }
}


// ************************************************************************* //
